<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ssh.entity.Clazz" table="class" schema="darkhome">
        <id name="classId">
            <column name="classId" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="className">
            <column name="className" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>