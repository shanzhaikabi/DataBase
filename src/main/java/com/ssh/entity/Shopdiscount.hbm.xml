<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ssh.entity.Shopdiscount" table="shopdiscount" schema="darkhome">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="discountType">
            <column name="discountType" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="shopId">
            <column name="shopId" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>