<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ssh.entity.Classdiscount" table="classdiscount" schema="darkhome">
        <id name="id">
            <column name="id" sql-type="int(20)"/>
        </id>
        <property name="discountType">
            <column name="discountType" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="classId">
            <column name="classId" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>