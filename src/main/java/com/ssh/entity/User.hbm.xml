<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ssh.entity.User" table="user" schema="mysys">
        <id name="userid" column="userid"/>
        <property name="name" column="name"/>
        <property name="password" column="password"/>
        <property name="identifyid" column="identifyid"/>
        <property name="qq" column="qq"/>
        <property name="wechat" column="wechat"/>
        <property name="address" column="address"/>
        <property name="tel" column="tel"/>
        <property name="phone" column="phone"/>
        <property name="email" column="email"/>
        <property name="homepage" column="homepage"/>
        <property name="identifytime" column="identifytime"/>
        <property name="teltime" column="teltime"/>
        <property name="mailtime" column="mailtime"/>
        <property name="status" column="status"/>
        <property name="createtime" column="createtime"/>
    </class>
</hibernate-mapping>